<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:gravity="center"
        android:layout_gravity="center"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:gravity="center"
            android:id="@+id/title"
            android:layout_marginLeft="@dimen/onfido_activity_horizontal_margin"
            android:layout_marginTop="4dp"
            android:layout_marginRight="@dimen/onfido_activity_horizontal_margin"
            android:text="@string/onfido_doc_select_section_input_country_not_found"
            android:layout_marginHorizontal="@dimen/onfido_activity_horizontal_margin"
            style="@style/OnfidoTextStyle.Body.Regular.Secondary"/>
    </LinearLayout>
</FrameLayout>
